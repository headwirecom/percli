#!/usr/bin/env node

var args    = require('commander')
var asciify = require('asciify')

args.version(require('../package.json').version)
	.command('install', 'install peregrine')
	.command('register', 'register server')
	.command('start', 'start peregrine')
	.command('stop', 'stop peregrine')
	.command('list', 'list all peregrine servers')
	.parse(process.argv)

